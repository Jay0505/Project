using System;
using System.Collections.Generic;
using System.Linq;

namespace Computersecurity
{
	class MainClass
	{
		public static void Main(string[] args)
		{
			Dictionary<double, double> dic = new Dictionary<double, double>();
			List<double> inputlist = new List<double>();
			Console.WriteLine("How many number of inputs?");
			int n = Convert.ToInt32(Console.ReadLine());
			var line = Console.ReadLine();
			var arguments = line.Split(' ');
			for (int i = 0; i < n; i++)
			{
	
				inputlist.Add(Convert.ToInt32(arguments[i]));
			}
			var average = inputlist.Average();
			foreach (int cw in inputlist)
			{
				double temp = aux_function(cw, average);
				dic.Add(cw, temp);
			}
			Console.WriteLine("Value-------------formulaic value");
			foreach (KeyValuePair<double, double> kvp in dic)
			{
				Console.WriteLine("\n-----------------------------------");
				Console.Write("\t{0}     -       {1}", kvp.Key, kvp.Value);
			}

		}

		public static double aux_function(double o, double avg)
		{
			double dif = o - avg;
			double numerator = dif * dif;
			double denominator = o;

			double div = numerator % denominator;

			return div;
		}
	}
}
